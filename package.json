{
    "name": "seedconnection-api",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "All Rights Reserved",
    "scripts": {
        "postinstall": "yarn run build",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "node dist/src/main.js",
        "start:dev": "nest start --watch",
        "dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/src/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --runInBand --detectOpenHandles --forceExit",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./jest-e2e.config.js --forceExit",
        "prettier": "prettier --write '**/*.{ts,js,css,html}'",
        "generate": "plop",
        "migrate:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/Database.ts migration:create -n",
        "migrate:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/Database.ts migration:generate -n",
        "migrate:up": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/Database.ts migration:run",
        "migrate:down": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/Database.ts migration:revert",
        "migrate:clear-cache": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/Database.ts cache:clear",
        "typeorm:drop-schema": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/Database.ts schema:drop"
    },
    "dependencies": {
        "@aginix/nestjs-firebase-admin": "^2.0.1",
        "@nestjs/common": "^7.0.0",
        "@nestjs/config": "^0.5.0",
        "@nestjs/core": "^7.0.0",
        "@nestjs/mongoose": "^7.2.2",
        "@nestjs/passport": "^7.1.0",
        "@nestjs/platform-express": "^7.4.4",
        "@nestjs/platform-socket.io": "^7.4.4",
        "@nestjs/schedule": "^0.4.1",
        "@nestjs/swagger": "^4.5.12",
        "@nestjs/websockets": "^7.4.4",
        "aws-sdk": "^2.756.0",
        "class-transformer": "^0.3.1",
        "class-transformer-validator": "^0.9.1",
        "class-validator": "^0.12.2",
        "date-fns": "^2.16.1",
        "dotenv": "^8.2.0",
        "firebase": "^7.24.0",
        "firebase-admin": "^9.2.0",
        "mongoose": "^5.11.14",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "swagger-ui-express": "^4.1.4"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.0.0",
        "@types/aws-sdk": "^2.7.0",
        "@types/bcrypt": "^3.0.0",
        "@types/date-fns": "^2.6.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.3",
        "@types/firebase": "^3.2.1",
        "@types/jest": "25.2.3",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.164",
        "@types/multer": "^1.4.4",
        "@types/multer-s3": "^2.7.8",
        "@types/node": "^13.9.1",
        "@types/nodemailer": "^6.4.0",
        "@types/nodemailer-mailgun-transport": "^1.4.1",
        "@types/passport": "^1.0.4",
        "@types/passport-jwt": "^3.0.3",
        "@types/socket.io": "^2.1.11",
        "@types/stripe": "^7.13.24",
        "@types/supertest": "^2.0.8",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/vimeo": "^2.1.2",
        "@typescript-eslint/eslint-plugin": "3.0.2",
        "@typescript-eslint/parser": "3.0.2",
        "eslint": "^7.9.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "26.0.1",
        "plop": "^2.7.4",
        "prettier": "^2.1.2",
        "supertest": "^4.0.2",
        "ts-jest": "26.1.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.4"
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "uploads"
    ]
}
